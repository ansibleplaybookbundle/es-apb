apiVersion: v1
kind: ImageStream
metadata:
  labels:
    app: {{ APPLICATION_NAME }}
  name: {{ APPLICATION_NAME }}
  namespace: {{ namespace }}
spec:
  lookupPolicy:
    local: false
  tags:
  - name: {{ elasticsearch_version }}
    from:
      kind: DockerImage
      name: registry.centos.org/rhsyseng/kibana:{{ elasticsearch_version }}
