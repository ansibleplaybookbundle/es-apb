apiVersion: v1
kind: ImageStream
metadata:
  labels:
    app: {{ application_name }}
  name: {{ application_name }}
  namespace: {{ namespace }}
spec:
  lookupPolicy:
    local: false
  tags:
  - name: {{ es_version }}
    from:
      kind: DockerImage
      name: {{ elasticsearch_image_name }}:{{ es_version }}
